#include <iostream>
#include <math.h>
using namespace std;

int main() 
{
    int i,j, A[10][10], B[10][10], x, y, C[10][10];
    for (i=1; i<=9; i++)
    {
        for (j=1; j<=9; j++)
        {
            A[i][j]=i*j;
            printf("%5d", A[i][j] );
        }
        printf("\n");
    }
    printf("\n");
    for (i=1; i<=9; i++)
    {
        for (j=1; j<=9; j++)
        {
            B[i][j]=i+j;
            printf("%5d", B[i][j] );
        }
        printf("\n");
    }
    printf("\n");
     for (i=1; i<=9; i++)
    {
        for (j=1; j<=9; j++)
        {
            C[i][j]=A[i][j]+B[i][j];
            printf("%5d", C[i][j] );
        }
        printf("\n");
    }
    
    return 0;
}
