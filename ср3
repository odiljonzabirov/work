//25.	Написать программу, которая по паролю будет определять уровень доступа сотрудника к секретной информации в базе данных. Доступ к базе имеют только шесть человек, разби-тых на три группы по степени доступа. Они имеют следующие пароли: 9583, 1747 — до-ступны модули баз А, В, С; 3331, 7922 — доступны модули баз В, С; 9455, 8997 — до-ступен модуль базы С.
#include <iostream>
#include <math.h>
using namespace std;

int main() 
{
   float x;
   cout << "Введите пароль = ";
   cin >> x;
   //Проверка верности пароля
   if (x == 9583 || x == 1747)
       cout << "Доступны модули баз A,B,C";
   else
   
       if (x == 3331 || x == 7922)
           cout <<"Доступны модули баз B,C";
       else
       
           if (x == 9455 || x == 8997)
               cout <<"Доступны модули баз C";
           else
               cout <<"Неправильный пароль";
}
